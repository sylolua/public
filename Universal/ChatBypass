local function DAPW_fake_script()
	local script = Instance.new('LocalScript', TextFrame)

	local TextChatService = game:GetService("TextChatService")

	local screenGui = script.Parent
	local textBox = screenGui:WaitForChild("MessageBox")
	local textButton = screenGui:WaitForChild("SendButton")
	local textButton2 = screenGui:WaitForChild("ClearButton")
	local imageButton1 = screenGui:WaitForChild("VerButton")
	local imageButton2 = screenGui:WaitForChild("PreButton")
	local imageButton3 = screenGui:WaitForChild("RbxButton")

	imageButton1.MouseButton1Click:Connect(function()
		textBox.Text = textBox.Text .. ""
	end)

	imageButton2.MouseButton1Click:Connect(function()
		textBox.Text = textBox.Text .. ""
	end)

	imageButton3.MouseButton1Click:Connect(function()
		textBox.Text = textBox.Text .. ""
	end)

	local function sendMessage()
		local message = textBox.Text
		if message ~= "" then
			local formattedText = message
			formattedText = formattedText:gsub("/ver/", "")
			formattedText = formattedText:gsub("/pre/", "")
			formattedText = formattedText:gsub("/rbx/", "")

			task.wait()

			local function SendMessageToChat(String)
				for _, Object in pairs(game:GetDescendants()) do
					if Object:IsA("RemoteEvent") and string.find(string.lower(Object.Name), "chat") then
						Object:FireServer(String)
					end
					if Object:IsA("RemoteFunction") and string.find(string.lower(Object.Name), "chat") then
						Object:InvokeServer(String)
					end
				end

				if not isLegacyChat then
					TextChatService.TextChannels.RBXGeneral:SendAsync(String)
				else
					ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer(String, "All")
				end
			end

			SendMessageToChat(formattedText)
		end
	end

	textButton.MouseButton1Click:Connect(sendMessage)

	textBox.FocusLost:Connect(function(enterPressed)
		if enterPressed then
			sendMessage()
		end
	end)

	textButton2.MouseButton1Click:Connect(function()
		textBox.Text = ""
	end)
end
coroutine.wrap(DAPW_fake_script)()
local function UZLPRH_fake_script()
	local script = Instance.new('LocalScript', CloseButton)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Visible = false
	end)
end
coroutine.wrap(UZLPRH_fake_script)()
local function EPZXV_fake_script() 
	local script = Instance.new('LocalScript', TextLabel)

	script.Parent.Active = true
	script.Parent.Draggable = true
end
coroutine.wrap(EPZXV_fake_script)()
